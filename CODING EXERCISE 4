package numericalComputationAndExpression_1;

public class formatingOutput {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int x = 10;
		double y = 5.373123, price = 17.29;
		
		System.out.printf("Value of x is %d and value of y is %4.2f%n", x , y);
		
		
		System.out.printf("%-10s%10.2f", "Total:", price);

	}

}
